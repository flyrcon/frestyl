              <!-- Header with Real-time Stats -->
              <div class="bg-white rounded-xl p-6 shadow-sm border">
                <div class="flex flex-col sm:flex-row sm:items-center justify-between mb-4">
                  <div>
                    <h2 class="text-2xl font-bold text-gray-900">Content Campaigns</h2>
                    <p class="text-gray-600 mt-1">Collaborative content creation with quality gates and revenue sharing</p>
                  </div>
                  
                  <!-- Real-time indicators -->
                  <div class="flex items-center space-x-4 mt-4 sm:mt-0">
                    <%= if length(@active_improvement_periods || []) > 0 do %>
                      <div class="flex items-center space-x-1 text-yellow-600">
                        <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M8.257 3.099c.765-1.36 2.722-1.36 3.486 0l5.58 9.92c.75 1.334-.213 2.98-1.742 2.98H4.42c-1.53 0-2.493-1.646-1.743-2.98l5.58-9.92zM11 13a1 1 0 11-2 0 1 1 0 012 0zm-1-8a1 1 0 00-1 1v3a1 1 0 002 0V6a1 1 0 00-1-1z" clip-rule="evenodd"/>
                        </svg>
                        <span class="text-sm font-medium"><%= length(@active_improvement_periods) %> improvement(s) needed</span>
                      </div>
                    <% end %>
                    
                    <%= if length(@pending_peer_reviews || []) > 0 do %>
                      <div class="flex items-center space-x-1 text-blue-600">
                        <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
                          <path d="M10 12a2 2 0 100-4 2 2 0 000 4z"/>
                          <path fill-rule="evenodd" d="M.458 10C1.732 5.943 5.522 3 10 3s8.268 2.943 9.542 7c-1.274 4.057-5.064 7-9.542 7S1.732 14.057.458 10zM14 10a4 4 0 11-8 0 4 4 0 018 0z" clip-rule="evenodd"/>
                        </svg>
                        <span class="text-sm font-medium"><%= length(@pending_peer_reviews) %> review(s) pending</span>
