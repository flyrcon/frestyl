                    <div class="text-xs text-white/70 mt-2">
                      <%= if @analytics_data.total_views > 0, do: "+12%", else: "0%" %> vs last period
                    </div>
                  </div>
                  
                  <div class="text-center p-6 rounded-xl bg-gradient-to-br from-emerald-500/90 via-teal-500/80 to-cyan-500/90">
                    <div class="text-3xl font-bold text-white drop-shadow-sm"><%= @analytics_data.published_count %></div>
                    <div class="text-sm text-white/80 mt-1">Published</div>
                    <div class="text-xs text-white/70 mt-2">
                      <%= @analytics_data.total_portfolios - @analytics_data.published_count %> drafts
                    </div>
                  </div>
                  
                  <div class="text-center p-6 rounded-xl bg-gradient-to-br from-purple-500/90 via-violet-500/80 to-indigo-500/90">
                    <div class="text-3xl font-bold text-white drop-shadow-sm"><%= @analytics_data.this_month_views %></div>
                    <div class="text-sm text-white/80 mt-1">This Month</div>
                    <div class="text-xs text-white/70 mt-2">
                      <%= if @analytics_data.this_month_views > 0, do: "+8%", else: "0%" %> vs last month
                    </div>
                  </div>
                  
                  <div class="text-center p-6 rounded-xl bg-gradient-to-br from-orange-500/90 via-red-500/80 to-pink-500/90">
                    <div class="text-3xl font-bold text-white drop-shadow-sm"><%= @analytics_data.engagement_rate %>%</div>
                    <div class="text-sm text-white/80 mt-1">Engagement</div>
                    <div class="text-xs text-white/70 mt-2">
                      <%= if @analytics_data.engagement_rate > 50, do: "Excellent", else: "Good" %>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Portfolio Performance -->
              <div class="bg-white rounded-xl p-6 shadow-sm border">
