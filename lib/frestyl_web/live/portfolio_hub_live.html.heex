                    <div class="w-3 h-3 bg-green-500 rounded-full"></div>
                    <span class="text-gray-600">Gmail connected</span>
                  </div>
                  <div class="flex items-center space-x-2">
                    <div class="w-3 h-3 bg-gray-300 rounded-full"></div>
                    <span class="text-gray-600">Outlook not connected</span>
                  </div>
                </div>
              </div>

              <!-- Calendar View Content -->
              <%= case @calendar_view do %>
                <% "day" -> %>
                  <!-- Day View -->
                  <div class="bg-white rounded-xl p-6 shadow-sm border">
                    <div class="flex items-center justify-between mb-6">
                      <h3 class="text-lg font-bold text-gray-900">Today - January 19, 2025</h3>
                      <div class="flex space-x-2">
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="prev">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"/>
                          </svg>
                        </button>
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="next">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                          </svg>
                        </button>
                      </div>
                    </div>
                    
                    <!-- Time Slots -->
                    <div class="space-y-2">
                      <%= for hour <- 9..17 do %>
                        <div class="flex items-center border-b border-gray-100 py-3">
                          <div class="w-16 text-sm text-gray-500 font-medium">
                            <%= if hour > 12, do: "#{hour-12}:00 PM", else: "#{hour}:00 AM" %>
                          </div>
                          <div class="flex-1 ml-4 min-h-[40px] hover:bg-gray-50 rounded cursor-pointer">
                            <%= if hour == 14 do %>
                              <div class="bg-gradient-to-r from-blue-500/10 to-cyan-500/10 border-l-4 border-blue-500 p-3 rounded">
                                <div class="font-medium text-gray-900">Portfolio Review</div>
                                <div class="text-sm text-gray-600">Review Q1 portfolio updates</div>
                              </div>
                            <% end %>
                          </div>
                        </div>
                      <% end %>
                    </div>
                  </div>

                <% "week" -> %>
                  <!-- Week View -->
                  <div class="bg-white rounded-xl p-6 shadow-sm border">
                    <div class="flex items-center justify-between mb-6">
                      <h3 class="text-lg font-bold text-gray-900">Week of January 19-25, 2025</h3>
                      <div class="flex space-x-2">
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="prev">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"/>
                          </svg>
                        </button>
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="next">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                          </svg>
                        </button>
                      </div>
                    </div>
                    
                    <!-- Week Grid -->
                    <div class="grid grid-cols-8 gap-px bg-gray-200 rounded-lg overflow-hidden">
                      <!-- Time Column -->
                      <div class="bg-gray-50"></div>
                      <%= for day <- ["Sun 19", "Mon 20", "Tue 21", "Wed 22", "Thu 23", "Fri 24", "Sat 25"] do %>
                        <div class="bg-gray-50 p-3 text-center text-sm font-medium text-gray-600">
                          <%= day %>
                        </div>
                      <% end %>
                      
                      <!-- Time Rows -->
                      <%= for hour <- 9..17 do %>
                        <div class="bg-white p-2 text-xs text-gray-500">
                          <%= if hour > 12, do: "#{hour-12}PM", else: "#{hour}AM" %>
                        </div>
                        <%= for _day <- 1..7 do %>
                          <div class="bg-white p-2 h-16 hover:bg-gray-50 transition-colors">
                            <%= if hour == 14 and _day == 3 do %>
                              <div class="bg-blue-100 text-blue-800 px-1 py-0.5 rounded text-xs truncate">
                                Review
                              </div>
                            <% end %>
                          </div>
                        <% end %>
                      <% end %>
                    </div>
                  </div>

                <% _ -> %>
                  <!-- Month View (Default) -->
                  <div class="bg-white rounded-xl p-6 shadow-sm border">
                    <div class="flex items-center justify-between mb-6">
                      <h3 class="text-lg font-bold text-gray-900">January 2025</h3>
                      <div class="flex space-x-2">
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="prev">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"/>
                          </svg>
                        </button>
                        <button class="p-2 hover:bg-gray-100 rounded-lg" phx-click="navigate_calendar" phx-value-direction="next">
                          <svg class="w-5 h-5 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                          </svg>
                        </button>
                      </div>
                    </div>
                    
                    <!-- Calendar Grid -->
                    <div class="grid grid-cols-7 gap-px bg-gray-200 rounded-lg overflow-hidden">
                      <!-- Headers -->
                      <%= for day <- ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"] do %>
                        <div class="bg-gray-50 p-3 text-center text-sm font-medium text-gray-600">
                          <%= day %>
                        </div>
                      <% end %>
                      
                      <!-- Calendar Days -->
                      <%= for day <- 1..31 do %>
                        <div class="bg-white p-3 h-24 hover:bg-gray-50 transition-colors relative cursor-pointer" phx-click="select_calendar_day" phx-value-day={day}>
                          <div class="text-sm font-medium text-gray-900"><%= day %></div>
                          <%= if day == 19 do %>
                            <div class="absolute bottom-1 left-1 right-1">
                              <div class="text-xs bg-blue-100 text-blue-800 px-1 py-0.5 rounded truncate">
                                Today
                              </div>
                            </div>
                          <% end %>
                          <%= if day == 22 do %>
                            <div class="absolute bottom-1 left-1 right-1">
                              <div class="text-xs bg-green-100 text-green-800 px-1 py-0.5 rounded truncate">
                                Review
                              </div>
                            </div>
                          <% end %>
                        </div>
                      <% end %>
                    </div>
                  </div>
              <% end %>
            </div>

          <% "content_campaigns" -> %>
            <!-- 📝 Enhanced Content Campaigns Tab with Phase 2 Features -->
