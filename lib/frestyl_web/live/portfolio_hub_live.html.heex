<!-- Navigation with Account Switcher -->
<.nav current_user={@current_user} active_tab={:portfolio_hub} />

<!-- Account Switcher integrated into navigation -->
<div class="pt-16 min-h-screen bg-gray-50">
  <!-- Account Switcher Bar -->
  <div class="bg-white border-b border-gray-200 px-4 sm:px-6 lg:px-8 py-3">
    <div class="max-w-7xl mx-auto flex items-center justify-between">
      <div class="flex items-center space-x-4">
        <h2 class="text-lg font-semibold text-gray-900">Portfolio Hub</h2>
        <span class="text-gray-500">•</span>
        <div class="relative">
          <!-- Current Account Display -->
          <button 
            phx-click="toggle_account_switcher"
            class="flex items-center space-x-2 bg-gray-50 hover:bg-gray-100 border border-gray-200 rounded-lg px-3 py-2 transition-colors">
            <div class={[
              "w-6 h-6 rounded-md flex items-center justify-center text-white text-xs font-bold bg-gradient-to-br",
              get_account_gradient(@current_account.type)
            ]}>
              <%= String.first(@current_account.name) %>
            </div>
            <span class="text-sm font-medium text-gray-700"><%= @current_account.name %></span>
            <svg class="w-4 h-4 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"/>
            </svg>
          </button>

          <!-- Account Switcher Dropdown -->
          <%= if @show_account_switcher do %>
            <div class="absolute top-full right-0 mt-2 w-80 bg-white rounded-xl shadow-xl border border-gray-200 py-4 z-50">
              <div class="px-4 pb-3 border-b border-gray-100">
                <h3 class="font-semibold text-gray-900">Switch Account</h3>
                <p class="text-sm text-gray-600">Select an account to switch to</p>
              </div>
              
              <!-- Available Accounts -->
              <div class="py-2 max-h-64 overflow-y-auto">
                <%= for account <- @available_accounts do %>
                  <button 
                    phx-click="switch_account" 
                    phx-value-account_id={account.id}
                    class={[
                      "w-full flex items-center space-x-3 px-4 py-3 hover:bg-gray-50 transition-colors text-left",
                      if(account.id == @current_account.id, do: "bg-blue-50 border-r-2 border-blue-500", else: "")
                    ]}>
                    <div class={[
                      "w-10 h-10 rounded-lg flex items-center justify-center text-white font-bold bg-gradient-to-br",
                      get_account_gradient(account.type)
                    ]}>
                      <%= String.first(account.name) %>
                    </div>
                    <div class="flex-1">
                      <div class="font-medium text-gray-900"><%= account.name %></div>
                      <div class="text-sm text-gray-600">
                        <%= format_account_type(account.type) %> • 
                        <%= format_subscription_tier(account.subscription_tier) %>
                      </div>
                    </div>
                    <%= if account.id == @current_account.id do %>
                      <svg class="w-5 h-5 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                      </svg>
                    <% end %>
                  </button>
                <% end %>
              </div>
              
              <!-- Add New Account -->
              <div class="px-4 pt-3 border-t border-gray-100">
                <button 
                  phx-click="show_create_modal" 
                  phx-value-type="account"
                  class="w-full flex items-center space-x-3 px-4 py-3 bg-gradient-to-r from-pink-600 to-purple-600 hover:from-pink-700 hover:to-purple-700 text-white rounded-lg transition-all">
                  <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"/>
                  </svg>
                  <span class="font-medium">Add New Account</span>
                </button>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
    <div class="grid grid-cols-12 gap-6">
      
      <!-- Left Sidebar -->
      <aside class="hidden lg:block col-span-3 space-y-6">
        <!-- Navigation Menu -->
        <div class="bg-white rounded-xl shadow-sm border">
          <!-- Dynamic Header Based on Active Tab -->
          <div class="p-6 border-b border-gray-200">
            <%= case @active_tab do %>
              <% "analytics" -> %>
                <h3 class="font-bold text-lg text-gray-900">Analytics Center</h3>
                <p class="text-sm text-gray-600 mt-1">Track your performance</p>
              <% "story_lab" -> %>
                <h3 class="font-bold text-lg text-gray-900">Story Lab</h3>
                <p class="text-sm text-gray-600 mt-1">Bring words to life</p>
              <% "collaboration_hub" -> %>
                <h3 class="font-bold text-lg text-gray-900">Collaboration Hub</h3>
                <p class="text-sm text-gray-600 mt-1">Work together seamlessly</p>
              <% "community_channels" -> %>
                <h3 class="font-bold text-lg text-gray-900">Community Channels</h3>
                <p class="text-sm text-gray-600 mt-1">Connect with creators</p>
              <% "content_campaigns" -> %>
                <h3 class="font-bold text-lg text-gray-900">Community Channels</h3>
                <p class="text-sm text-gray-600 mt-1">Connect with creators</p>
              <% "service_dashboard" -> %>
                <h3 class="font-bold text-lg text-gray-900">Service Dashboard</h3>
                <p class="text-sm text-gray-600 mt-1">Monetize your expertise</p>
              <% "creator_studio" -> %>
                <h3 class="font-bold text-lg text-gray-900">Creator Studio</h3>
                <p class="text-sm text-gray-600 mt-1">Professional creation tools</p>
              <% "settings" -> %>
                <h3 class="font-bold text-lg text-gray-900">Settings</h3>
                <p class="text-sm text-gray-600 mt-1">Configure preferences</p>
              <% _ -> %>
                <h3 class="font-bold text-lg text-gray-900">Portfolio Hub</h3>
                <p class="text-sm text-gray-600 mt-1">Manage your creative work</p>
            <% end %>
          </div>
          
          <nav class="p-4">
            <div class="space-y-1">
              <!-- Portfolio Hub -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="portfolio_hub"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "portfolio_hub", do: "bg-gradient-to-r from-gray-900 to-gray-800 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"/>
                </svg>
                <span>Portfolio Hub</span>
              </button>

              <!-- Collaboration Hub -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="collaboration_hub"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "collaboration_hub", do: "bg-gradient-to-r from-orange-600 to-red-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"/>
                </svg>
                <span>Collaboration Hub</span>
              </button>

              <!-- Content Campaigns Tab -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="content_campaigns"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "content_campaigns", do: "bg-gradient-to-r from-purple-600 to-pink-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"/>
                </svg>
                <span>Content Campaigns</span>
              </button>

              <!-- Community Channels -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="community_channels"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "community_channels", do: "bg-gradient-to-r from-indigo-600 to-purple-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 4V2a1 1 0 011-1h8a1 1 0 011 1v2m-9 4v10a2 2 0 002 2h6a2 2 0 002-2V8M9 8h6"/>
                </svg>
                <span>Community Channels</span>
              </button>

              <!-- Story Lab/Engine -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="story_lab"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "story_lab", do: "bg-gradient-to-r from-purple-600 to-pink-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15.232 5.232l3.536 3.536m-2.036-5.036a2.5 2.5 0 113.536 3.536L6.5 21.036H3v-3.572L16.732 3.732z"/>
                </svg>
                <span>Story Engine</span>
              </button>

              <!-- Analytics -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="analytics"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "analytics", do: "bg-gradient-to-r from-blue-600 to-cyan-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
                </svg>
                <span>Analytics</span>
              </button>

              <!-- Calendar -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="calendar"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "calendar", do: "bg-gradient-to-r from-emerald-600 to-teal-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"/>
                </svg>
                <span>Calendar</span>
              </button>

              <!-- Service Dashboard (Creator Tier+) -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="service_dashboard"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "service_dashboard", do: "bg-gradient-to-r from-emerald-600 to-green-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1"/>
                </svg>
                <span>Service Dashboard</span>
                <%= unless can_access_feature?(@current_account, :service_booking) do %>
                  <span class="ml-auto">
                    <svg class="w-4 h-4 text-amber-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"/>
                    </svg>
                  </span>
                <% end %>
              </button>

              <!-- Creator Studio (Creator Tier+) -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="creator_studio"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "creator_studio", do: "bg-gradient-to-r from-violet-600 to-purple-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 10l4.553-2.276A1 1 0 0121 8.618v6.764a1 1 0 01-1.447.894L15 14M5 18h8a2 2 0 002-2V8a2 2 0 00-2-2H5a2 2 0 00-2 2v8a2 2 0 002 2z"/>
                </svg>
                <span>Creator Studio</span>
                <%= unless can_access_feature?(@current_account, :creator_studio) do %>
                  <span class="ml-auto">
                    <svg class="w-4 h-4 text-amber-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"/>
                    </svg>
                  </span>
                <% end %>
              </button>

              <!-- Settings -->
              <button 
                phx-click="switch_tab" 
                phx-value-tab="settings"
                class={[
                  "w-full flex items-center space-x-3 px-3 py-2 rounded-lg text-sm font-medium transition-colors",
                  if(@active_tab == "settings", do: "bg-gradient-to-r from-slate-600 to-gray-600 text-white shadow-sm", else: "text-gray-700 hover:bg-gray-100")
                ]}>
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"/>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
                </svg>
                <span>Settings</span>
              </button>
            </div>
          </nav>
        </div>

        <!-- Enhanced Quick Stats (preserved from original) -->
        <div class="bg-white rounded-xl p-6 shadow-sm border">
          <h3 class="font-bold text-lg text-gray-900 mb-4">Quick Stats</h3>
          <div class="space-y-4">
            <div class="flex items-center justify-between p-3 bg-gradient-to-r from-blue-50 to-cyan-50 rounded-lg">
              <div class="flex items-center space-x-3">
                <div class="w-2 h-8 bg-gradient-to-br from-blue-500 to-cyan-500 rounded-full"></div>
                <span class="text-gray-700 font-medium text-sm">Portfolios</span>
              </div>
              <span class="font-mono font-bold text-gray-900"><%= @analytics_data.total_portfolios %></span>
            </div>
            
            <div class="flex items-center justify-between p-3 bg-gradient-to-r from-emerald-50 to-teal-50 rounded-lg">
              <div class="flex items-center space-x-3">
                <div class="w-2 h-8 bg-gradient-to-br from-emerald-500 to-teal-500 rounded-full"></div>
                <span class="text-gray-700 font-medium text-sm">Published</span>
              </div>
              <span class="font-mono font-bold text-gray-900"><%= @analytics_data.published_count %></span>
            </div>
            
            <div class="flex items-center justify-between p-3 bg-gradient-to-r from-purple-50 to-pink-50 rounded-lg">
              <div class="flex items-center space-x-3">
                <div class="w-2 h-8 bg-gradient-to-br from-purple-500 to-pink-500 rounded-full"></div>
                <span class="text-gray-700 font-medium text-sm">Total Views</span>
              </div>
              <span class="font-mono font-bold text-gray-900"><%= @analytics_data.total_views %></span>
            </div>
            
            <div class="flex items-center justify-between p-3 bg-gradient-to-r from-orange-50 to-red-50 rounded-lg">
              <div class="flex items-center space-x-3">
                <div class="w-2 h-8 bg-gradient-to-br from-orange-500 to-red-500 rounded-full"></div>
                <span class="text-gray-700 font-medium text-sm">This Month</span>
              </div>
              <span class="font-mono font-bold text-gray-900"><%= @analytics_data.this_month_views %></span>
            </div>
          </div>
        </div>

        <!-- Dynamic Quick Actions Based on Active Tab (enhanced with new tabs) -->
        <div class="bg-white rounded-xl p-6 shadow-sm border">
          <h3 class="font-bold text-lg text-gray-900 mb-4">Quick Actions</h3>
          <div class="space-y-3">
            <%= case @active_tab do %>
              <% "collaboration_hub" -> %>
                <!-- Collaboration Quick Actions -->
                <button 
                  phx-click="show_create_modal" 
                  phx-value-type="collaboration"
                  class="w-full flex items-center space-x-3 p-3 bg-gradient-to-r from-orange-50 to-red-50 hover:from-orange-100 hover:to-red-100 rounded-xl group transition-all">
                  <div class="w-10 h-10 bg-gradient-to-br from-orange-600 to-red-600 rounded-lg flex items-center justify-center group-hover:scale-105 transition-transform">
                    <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"/>
                    </svg>
                  </div>
                  <div class="text-left">
                    <div class="font-medium text-gray-900">Start Collaboration</div>
                    <div class="text-sm text-gray-600">Invite team members</div>
                  </div>
                </button>

                <button 
                  phx-click="show_create_modal" 
                  phx-value-type="invite"
                  class="w-full flex items-center space-x-3 p-3 bg-gradient-to-r from-purple-50 to-pink-50 hover:from-purple-100 hover:to-pink-100 rounded-xl group transition-all">
                  <div class="w-10 h-10 bg-gradient-to-br from-purple-600 to-pink-600 rounded-lg flex items-center justify-center group-hover:scale-105 transition-transform">
                    <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 4.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"/>
                    </svg>
                  </div>
                  <div class="text-left">
                    <div class="font-medium text-gray-900">Send Invites</div>
                    <div class="text-sm text-gray-600">Invite collaborators</div>
                  </div>
                </button>

              <% "community_channels" -> %>
                <!-- Community Channels Quick Actions -->
                <button 
                  phx-click="show_create_modal" 
                  phx-value-type="channel"
                  class="w-full flex items-center space-x-3 p-3 bg-gradient-to-r from-indigo-50 to-purple-50 hover:from-indigo-100 hover:to-purple-100 rounded-xl group transition-all">
                  <div class="w-10 h-10 bg-gradient-to-br from-indigo-600 to-purple-600 rounded-lg flex items-center justify-center group-hover:scale-105 transition-transform">
                    <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 4V2a1 1 0 011-1h8a1 1 0 011 1v2m-9 4v10a2 2 0 002 2h6a2 2 0 002-2V8M9 8h6"/>
                    </svg>
                  </div>
                  <div class="text-left">
                    <div class="font-medium text-gray-900">Create Channel</div>
                    <div class="text-sm text-gray-600">Start a new community</div>
                  </div>
                </button>

                <button 
                  phx-click="start_session" 
                  phx-value-channel_id="1"
