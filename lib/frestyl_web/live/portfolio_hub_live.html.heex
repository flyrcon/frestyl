                <div class="space-y-4">
                  <%= for {portfolio, index} <- Enum.with_index(@portfolios) do %>
                    <div class="flex items-center justify-between p-4 bg-gray-50 rounded-lg hover:bg-gray-100 transition-colors">
                      <div class="flex items-center space-x-4">
                        <div class={[
                          "w-3 h-8 rounded-full bg-gradient-to-br",
                          get_portfolio_icon_gradient(portfolio)
                        ]}>
                        </div>
                        <div>
                          <h4 class="font-semibold text-gray-900"><%= portfolio.title %></h4>
                          <p class="text-sm text-gray-600">Updated <%= time_ago(portfolio.updated_at) %></p>
                        </div>
                      </div>
                      
                      <div class="flex items-center space-x-6 text-sm">
                        <div class="text-center">
                          <div class="font-bold text-gray-900"><%= get_portfolio_views(portfolio) %></div>
                          <div class="text-gray-500">Views</div>
                        </div>
                        <div class="text-center">
                          <div class="font-bold text-gray-900"><%= calculate_completion_percentage(portfolio) %>%</div>
                          <div class="text-gray-500">Complete</div>
                        </div>
                        <div class="text-center">
                          <span class={[
                            "inline-flex items-center px-2.5 py-1 text-xs font-semibold rounded-full",
                            get_status_badge_classes(portfolio.visibility)
                          ]}>
                            <%= format_visibility(portfolio.visibility) %>
                          </span>
                        </div>
                      </div>
                    </div>
                  <% end %>
                </div>
              </div>

              <!-- Quick Insights -->
              <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
                <div class="bg-white rounded-xl p-6 shadow-sm border">
                  <h3 class="text-lg font-bold text-gray-900 mb-4">Top Performing</h3>
                  <%= if length(@portfolios) > 0 do %>
                    <% top_portfolio = Enum.max_by(@portfolios, &get_portfolio_views/1) %>
                    <div class="flex items-center space-x-4 p-4 bg-gradient-to-r from-green-50 to-emerald-50 rounded-lg">
                      <div class="w-2 h-12 bg-gradient-to-br from-emerald-500 to-green-500 rounded-full"></div>
                      <div>
                        <h4 class="font-semibold text-gray-900"><%= top_portfolio.title %></h4>
                        <p class="text-sm text-emerald-600 font-medium"><%= get_portfolio_views(top_portfolio) %> views</p>
                      </div>
                    </div>
                  <% else %>
                    <p class="text-gray-500 text-center py-8">No portfolios yet</p>
                  <% end %>
                </div>
                
                <div class="bg-white rounded-xl p-6 shadow-sm border">
                  <h3 class="text-lg font-bold text-gray-900 mb-4">Recent Activity</h3>
                  <div class="space-y-3">
                    <%= for portfolio <- Enum.take(@portfolios, 3) do %>
                      <div class="flex items-center space-x-3 text-sm">
                        <div class="w-2 h-2 bg-blue-500 rounded-full"></div>
                        <span class="text-gray-600">Updated</span>
                        <span class="font-medium text-gray-900"><%= portfolio.title %></span>
                        <span class="text-gray-500"><%= time_ago(portfolio.updated_at) %></span>
                      </div>
                    <% end %>
                    <%= if length(@portfolios) == 0 do %>
                      <p class="text-gray-500 text-center py-4">No recent activity</p>
                    <% end %>
                  </div>
                </div>
              </div>
            </div>
          
