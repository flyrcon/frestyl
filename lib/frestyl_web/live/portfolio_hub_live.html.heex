                        class={[
                          "flex items-center px-3 py-2 text-sm rounded-md transition-all",
                          if(@view_mode == "list", do: "bg-white text-gray-900 shadow-sm", else: "text-gray-600 hover:text-gray-900")
                        ]}>
                        <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 10h16M4 14h16M4 18h16"/>
                        </svg>
                        List
                      </button>
                      <button 
                        phx-click="change_view" 
                        phx-value-view="cards"
                        class={[
                          "flex items-center px-3 py-2 text-sm rounded-md transition-all",
                          if(@view_mode == "cards", do: "bg-white text-gray-900 shadow-sm", else: "text-gray-600 hover:text-gray-900")
                        ]}>
                        <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2H6a2 2 0 01-2-2V6zM14 6a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2V6zM4 16a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2H6a2 2 0 01-2-2v-2zM14 16a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2v-2z"/>
                        </svg>
                        Cards
                      </button>
                    </div>

                    <!-- Sort & Filter -->
                    <select 
                      phx-change="change_sort" 
                      name="sort_by"
                      class="rounded-lg border-gray-300 text-sm focus:border-gray-500 focus:ring-gray-500">
                      <option value="recent" selected={@sort_by == "recent"}>Recently Updated</option>
                      <option value="name" selected={@sort_by == "name"}>Name A-Z</option>
                      <option value="views" selected={@sort_by == "views"}>Most Views</option>
                      <option value="status" selected={@sort_by == "status"}>Status</option>
                    </select>

                    <select 
                      phx-change="change_filter" 
                      name="filter"
                      class="rounded-lg border-gray-300 text-sm focus:border-gray-500 focus:ring-gray-500">
                      <option value="all" selected={@filter_status == "all"}>All Portfolios</option>
                      <option value="published" selected={@filter_status == "published"}>Published</option>
                      <option value="draft" selected={@filter_status == "draft"}>Drafts</option>
                      <option value="private" selected={@filter_status == "private"}>Private</option>
                    </select>
                  </div>

                  <span class="text-sm text-gray-600"><%= length(@portfolios) %> portfolios</span>
                </div>
              </div>

              <!-- Portfolio Grid/List (preserved from original with existing styling) -->
              <%= if @view_mode == "cards" do %>
                <!-- Card View -->
                <div class="grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-6">
                  <%= for portfolio <- sort_portfolios(filter_portfolios(@portfolios, @filter_status), @sort_by) do %>
                    <div class="group bg-white rounded-xl shadow-sm border border-gray-200 hover:shadow-lg hover:border-gray-300 transition-all duration-300 overflow-hidden">
                      <!-- Card Header -->
                      <div class={[
                        "relative p-4 border-b border-gray-100 bg-gradient-to-br",
                        get_portfolio_card_gradient(portfolio)
                      ]}>
                        <!-- Status Badge -->
                        <div class="absolute top-3 right-3">
                          <span class={[
                            "inline-flex items-center px-2.5 py-1 text-xs font-semibold rounded-full",
                            get_status_badge_classes(portfolio.visibility)
                          ]}>
                            <%= format_visibility(portfolio.visibility) %>
                          </span>
                        </div>

                        <!-- Portfolio Title - Left Aligned -->
                        <div class="text-left pr-16">
                          <h3 class="font-bold text-white text-lg mb-1 line-clamp-1 drop-shadow-sm"><%= portfolio.title %></h3>
                          <p class="text-sm text-white/80">Updated <%= time_ago(portfolio.updated_at) %></p>
                        </div>
                      </div>

                      <!-- Card Content -->
                      <div class="p-6">
                        <!-- Description -->
                        <p class="text-sm text-gray-600 line-clamp-2 leading-relaxed mb-4">
                          <%= portfolio.description || "No description provided" %>
                        </p>

                        <!-- Stats Grid -->
                        <div class="grid grid-cols-3 gap-4 mb-6">
                          <!-- Views -->
                          <div class="text-center">
                            <div class="text-lg font-bold text-gray-900"><%= get_portfolio_views(portfolio) %></div>
                            <div class="text-xs text-gray-500">Views</div>
                          </div>
                          
                          <!-- Sections -->
                          <div class="text-center">
                            <div class="text-lg font-bold text-gray-900"><%= get_portfolio_sections_count(portfolio) %></div>
                            <div class="text-xs text-gray-500">Sections</div>
                          </div>

                          <!-- Completion -->
                          <div class="text-center">
                            <div class="text-lg font-bold text-gray-900"><%= calculate_completion_percentage(portfolio) %>%</div>
                            <div class="text-xs text-gray-500">Complete</div>
                          </div>
                        </div>

                        <!-- Action Buttons -->
                        <div class="flex items-center justify-between">
                          <!-- Primary Actions -->
                          <div class="flex space-x-2">
                            <.link 
                              navigate={~p"/portfolios/#{portfolio.id}/edit"} 
                              class="bg-gray-900 hover:bg-gray-800 text-white px-3 py-1 rounded-md text-sm font-medium transition-colors">
                              Edit
                            </.link>
                            
                            <.link 
                              navigate={~p"/portfolios/#{portfolio.id}"} 
                              target="_blank"
                              class="bg-gray-100 hover:bg-gray-200 text-gray-700 px-3 py-1 rounded-md text-sm font-medium transition-colors">
                              View
                            </.link>
                          </div>

                          <!-- Icon Actions -->
                          <div class="flex items-center space-x-1">
                            <!-- Video Call Button -->
                            <button 
                              title="Start video call"
                              class="p-2 text-gray-400 hover:text-blue-600 hover:bg-blue-50 rounded-lg transition-colors">
                              <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 10l4.553-2.276A1 1 0 0121 8.618v6.764a1 1 0 01-1.447.894L15 14M5 18h8a2 2 0 002-2V8a2 2 0 00-2-2H5a2 2 0 00-2 2v8a2 2 0 002 2z"/>
                              </svg>
                            </button>

                            <!-- More Options -->
                            <div class="relative">
                              <button class="p-2 text-gray-400 hover:text-gray-600 hover:bg-gray-50 rounded-lg transition-colors">
                                <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 5v.01M12 12v.01M12 19v.01M12 6a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2z"/>
                                </svg>
                              </button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  <% end %>
                </div>
              <% else %>
                <!-- List View -->
                <div class="bg-white rounded-xl shadow-sm border">
                  <div class="divide-y divide-gray-200">
                    <%= for portfolio <- sort_portfolios(filter_portfolios(@portfolios, @filter_status), @sort_by) do %>
                      <div class="p-6 hover:bg-gray-50 transition-colors">
                        <div class="flex items-center justify-between">
                          <div class="flex items-center space-x-4">
                            <!-- Portfolio Icon -->
                            <div class={[
                              "w-1 h-12 rounded-full bg-gradient-to-br",
                              get_portfolio_icon_gradient(portfolio)
                            ]}>
                            </div>

                            <!-- Portfolio Info -->
                            <div class="flex-1 min-w-0">
                              <div class="flex items-center space-x-2">
                                <h3 class="font-semibold text-gray-900"><%= portfolio.title %></h3>
                                <span class={[
                                  "inline-flex items-center px-2.5 py-1 text-xs font-semibold rounded-full",
                                  get_status_badge_classes(portfolio.visibility)
                                ]}>
                                  <%= format_visibility(portfolio.visibility) %>
                                </span>
                              </div>
                              <p class="text-sm text-gray-600 mt-1 line-clamp-1">
                                <%= portfolio.description || "No description provided" %>
                              </p>
                              <div class="flex items-center space-x-4 mt-2 text-xs text-gray-500">
                                <span><%= get_portfolio_views(portfolio) %> views</span>
                                <span><%= get_portfolio_sections_count(portfolio) %> sections</span>
                                <span><%= calculate_completion_percentage(portfolio) %>% complete</span>
                                <span>Updated <%= time_ago(portfolio.updated_at) %></span>
                              </div>
                            </div>
                          </div>

                          <!-- Actions -->
                          <div class="flex items-center space-x-2">
                            <.link 
                              navigate={~p"/portfolios/#{portfolio.id}/edit"} 
                              class="bg-gray-900 hover:bg-gray-800 text-white px-3 py-1 rounded-md text-sm font-medium">
                              Edit
                            </.link>
                            <.link 
                              navigate={~p"/portfolios/#{portfolio.id}"} 
                              target="_blank"
                              class="bg-gray-100 hover:bg-gray-200 text-gray-700 px-3 py-1 rounded-md text-sm font-medium">
                              View
                            </.link>
                          </div>
                        </div>
                      </div>
                    <% end %>
                  </div>
                </div>
              <% end %>
            </div>

              <!-- Include all other original tabs: story_lab, analytics, calendar, settings -->
              <!-- [Previous tab content from original template preserved here] -->
        <% end %>

      </main>
    </div>
  </div>
</div>

<!-- Enhanced Create Modal with Account Support -->
<%= if @show_create_modal do %>
  <div class="fixed inset-0 bg-gray-600 bg-opacity-50 overflow-y-auto h-full w-full z-50">
    <div class="relative top-20 mx-auto p-5 border w-96 shadow-lg rounded-md bg-white">
      <div class="mt-3">
        <div class="flex justify-between items-center mb-4">
          <h3 class="text-lg font-medium text-gray-900">
            Create New <%= String.capitalize(@create_type || "Item") %>
          </h3>
          <button 
            phx-click="close_create_modal"
            class="text-gray-400 hover:text-gray-600">
            <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"/>
            </svg>
          </button>
        </div>
        
        <form phx-submit={
          case @create_type do
            "portfolio" -> "create_portfolio"
            "story" -> "create_story"
            "collaboration" -> "start_collaboration"
            "channel" -> "create_channel"
            "service" -> "create_service"
            "account" -> "create_account"
            _ -> "create_portfolio"
          end
        }>
          <div class="space-y-4">
            <!-- Account Type (for account creation) -->
            <%= if @create_type == "account" do %>
              <div>
                <label class="block text-sm font-medium text-gray-700 mb-2">Account Type</label>
                <select 
                  name="type" 
                  required
                  class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500">
                  <option value="personal">Personal</option>
                  <option value="work">Work</option>
                  <option value="team">Team</option>
                </select>
              </div>
            <% end %>

            <!-- Title/Name field -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">
                <%= case @create_type do %>
                  <% "account" -> %>Account Name
                  <% "channel" -> %>Channel Name
                  <% "service" -> %>Service Title
                  <% _ -> %>Title
                <% end %>
              </label>
              <input 
                type="text" 
                name={if @create_type == "account", do: "name", else: "title"}
                required
                class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500"
                placeholder={
                  case @create_type do
                    "account" -> "My Work Account"
                    "channel" -> "Creative Community"
                    "service" -> "Design Consultation"
                    "collaboration" -> "Brand Redesign Project"
                    _ -> "My New Portfolio"
                  end
                } />
            </div>
            
            <!-- Description field -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">Description</label>
              <textarea 
                name="description" 
                rows="3"
                class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500"
                placeholder={
                  case @create_type do
                    "channel" -> "A space for creative minds to connect..."
                    "service" -> "Professional design consultation services..."
                    "collaboration" -> "Working together on a complete brand refresh..."
                    _ -> "Brief description..."
                  end
                }></textarea>
            </div>

            <!-- Conditional fields based on creation type -->
            <%= case @create_type do %>
              <% "portfolio" -> %>
                <div>
                  <label class="block text-sm font-medium text-gray-700 mb-2">Visibility</label>
                  <select name="visibility" class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500">
                    <option value="private">Private</option>
                    <option value="public">Public</option>
                  </select>
                </div>

              <% "channel" -> %>
                <div class="flex items-center">
                  <input 
                    type="checkbox" 
                    id="is_public" 
                    name="is_public" 
                    value="true"
                    class="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded">
                  <label for="is_public" class="ml-2 block text-sm text-gray-900">
                    Make channel public
                  </label>
                </div>

              <% "service" -> %>
                <div class="grid grid-cols-2 gap-3">
                  <div>
                    <label class="block text-sm font-medium text-gray-700 mb-2">Price</label>
                    <div class="relative">
                      <span class="absolute left-3 top-2 text-gray-500">$</span>
                      <input 
                        type="number" 
                        name="price" 
                        min="0"
                        step="0.01"
                        placeholder="99.00"
                        class="w-full pl-7 pr-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500">
                    </div>
                  </div>
                  <div>
                    <label class="block text-sm font-medium text-gray-700 mb-2">Duration</label>
                    <select name="duration" class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500">
                      <option value="1_hour">1 Hour</option>
                      <option value="2_hours">2 Hours</option>
                      <option value="half_day">Half Day</option>
                      <option value="full_day">Full Day</option>
                      <option value="custom">Custom</option>
                    </select>
                  </div>
                </div>

              <% "collaboration" -> %>
                <div>
                  <label class="block text-sm font-medium text-gray-700 mb-2">Team Members (Email)</label>
                  <input 
                    type="email" 
                    name="collaborator_email" 
                    placeholder="colleague@example.com"
                    class="w-full px-3 py-2 border border-gray-300 rounded-md focus:ring-gray-500 focus:border-gray-500">
                  <p class="text-xs text-gray-500 mt-1">You can invite more members after creation</p>
                </div>

              <% _ -> %>
                <!-- Default fields for other types -->
            <% end %>
          </div>
          
          <div class="flex justify-end space-x-3 mt-6">
            <button 
              type="button"
              phx-click="close_create_modal"
              class="px-4 py-2 border border-gray-300 text-gray-700 rounded-md hover:bg-gray-50">
              Cancel
            </button>
            <button 
              type="submit"
              class={[
                "px-4 py-2 text-white rounded-md font-medium",
                case @create_type do
                  "collaboration" -> "bg-gradient-to-r from-orange-600 to-red-600 hover:from-orange-700 hover:to-red-700"
                  "channel" -> "bg-gradient-to-r from-indigo-600 to-purple-600 hover:from-indigo-700 hover:to-purple-700"
                  "service" -> "bg-gradient-to-r from-emerald-600 to-green-600 hover:from-emerald-700 hover:to-green-700"
                  "account" -> "bg-gradient-to-r from-pink-600 to-purple-600 hover:from-pink-700 hover:to-purple-700"
                  _ -> "bg-gray-900 hover:bg-gray-800"
                end
              ]}>
              Create <%= String.capitalize(@create_type || "Item") %>
            </button>
          </div>
        </form>
      </div>
    </div>
  </div>
<% end %>

